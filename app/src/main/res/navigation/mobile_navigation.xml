<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.rk.electronic.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.rk.electronic.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.rk.electronic.ui.supplier.SupplierFragment"
        android:label="@string/title_supplier"
        tools:layout="@layout/fragment_supplier" />

    <fragment
        android:id="@+id/navigation_blank"
        android:name="com.rk.electronic.ui.driver.DriverFragment"
        android:label="@string/title_driver"
        tools:layout="@layout/fragment_driver" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.rk.electronic.ui.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />
</navigation>